<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NET.XF.Intro.View.Home.LoginView">
  <!--<Label Text="{Binding MainText}" VerticalOptions="Center" HorizontalOptions="Center" />-->
  <ContentPage.Content>
    <StackLayout Padding="30">
      <Label Text="Autenticação" Font="20" />
      <Editor x:Name="txtUser" Placeholder="Usuário" />
      <Editor x:Name="txtPassword"  Placeholder="Senha" />
      <Button x:Name="btnEnter" Text="Entrar" Clicked="OnEnter_Clicked" />
    </StackLayout>
  </ContentPage.Content>


</ContentPage>